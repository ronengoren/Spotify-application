

<body onload="displayCalendar()"> 

<div id="calendargrid">
 


  

</div>


  <!-- Modal -->
  <div class="modal" id="myModal" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title"></h4>
        </div>
        <div class="modal-body">
          <p><div class='control-group'>
            <%= form_for Event.create, url: calendars_path, method: :post do |f| %>
            <%= f.label :start_date, "Start Date :", class: 'control-label' %>
        <div class='controls'>
            <%= f.text_field :start_date, class: 'span11', id: 'start_date', autofocus: true %>
            </div>
             </div>
      <div class='control-group'>
        <%= f.label :end_date, 'End Date :', class: 'control-label' %>
        <div class='controls'>
      <%= f.text_field :end_date, class: 'span11', id: 'end_date',  autofocus: true, placeholder: 'mm/dd/yyyy' %>
        </div>
      </div>
       <div class='control-group'>
        <%= f.label :description, 'Description :', class: 'control-label' %>
        <div class="controls">
      <%= f.text_field :description, class: 'span11', autofocus: true, placeholder: 'Description' %>
        </div>
      </div>

      <div class='form-actions'>
      <%= f.submit :Submit, class: 'btn btn-success'%>
        <% end %>
      </div></p>

        
          
<div class="container">
    <div class="track"></div>
  <div class="artist"></div>
  <div class="album"></div>

  <a>spotify link<a/>
  <span></span>
</div>



<%= @specificdate.each do |start_dates| %>
<li> <%= start_dates.start_date %> </li>
<li> <%= start_dates.description %> </li>
<% end %>



        </div>
        <div class="modal-footer">
          <button type="button" id="closemodal" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
      
    </div>
  </div>
